<h1>List of usernames:</h1>
<table>
  <thead>
    <tr>
      <th>Username:</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |u| %>
      <tr>
        <td><%= link_to u.username, user_path(u.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Create a new user', new_user_path %>
